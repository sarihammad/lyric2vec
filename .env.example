# Model Configuration
HF_TEXT_MODEL=distilbert-base-uncased
AUDIO_SR=16000
EMB_D=256
LOSS_WEIGHTS=1.0,0.5,0.2

# FAISS Configuration
FAISS_TYPE=HNSW
FAISS_M=32
FAISS_EF_SEARCH=128

# API Configuration
API_PORT=8080
API_HOST=0.0.0.0
PROM_PATH=/metrics

# Training Configuration
BATCH_SIZE=32
LEARNING_RATE=1e-4
NUM_EPOCHS=10
FP16=true

# Ray Configuration
RAY_NUM_WORKERS=4
RAY_USE_GPU=false

# Data Paths
DATA_RAW_PATH=data/raw
DATA_PROCESSED_PATH=data/processed
DATA_ARTIFACTS_PATH=data/artifacts

# Logging
LOG_LEVEL=INFO
